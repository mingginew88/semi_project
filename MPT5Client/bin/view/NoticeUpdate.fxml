<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="board.notice.NoticeUpdateController">
   <children>
      <BorderPane prefHeight="760.0" prefWidth="1200.0" style="-fx-background-color: white;">
         <top>
            <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <Label prefHeight="70.0" prefWidth="780.0" style="-fx-font-size: 20; -fx-background-color: white;" text="◎게시판 &gt; 공지사항 &gt; 수정하기">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </top>
         <bottom>
            <HBox alignment="CENTER" prefHeight="31.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="cancelButton" maxHeight="30.0" maxWidth="70.0" minHeight="30.0" minWidth="50.0" mnemonicParsing="false" onAction="#cancelButtonAct" prefHeight="30.0" prefWidth="50.0" text="취소">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="saveButton" maxHeight="30.0" maxWidth="50.0" minHeight="30.0" minWidth="50.0" mnemonicParsing="false" onAction="#saveButtonAct" prefHeight="30.0" prefWidth="50.0" text="저장">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </bottom>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="800.0">
                     <children>
                        <Label style="-fx-font-size: 15; -fx-font-style: bold;" text="제목">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="titleText" prefHeight="23.0" prefWidth="1106.0">
                           <HBox.margin>
                              <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox prefHeight="705.0" prefWidth="1200.0">
                     <children>
                        <Label style="-fx-font-size: 15;" text="내용">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <TextArea fx:id="contArea" prefHeight="603.0" prefWidth="1107.0">
                           <HBox.margin>
                              <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </TextArea>
                     </children>
                  </HBox>
                  <HBox prefHeight="82.0" prefWidth="800.0" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</StackPane>
